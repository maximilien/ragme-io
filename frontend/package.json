{
  "name": "ragme-io-frontend",
  "version": "1.0.0",
  "type": "module",
  "description": "Modern frontend UI for RAGme.io Assistant",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "watch": "tsc --watch",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "lint:ci": "npx eslint src/**/*.ts || echo 'ESLint failed, but continuing...'",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "ragme",
    "rag",
    "ai",
    "assistant",
    "typescript",
    "nodejs"
  ],
  "author": "RAGme.io team",
  "license": "MIT",
  "dependencies": {
    "@eslint/js": "^9.33.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "axios": "^1.11.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dompurify": "^3.0.5",
    "eslint": "^9.33.0",
    "express": "^4.18.2",
    "form-data": "^4.0.4",
    "helmet": "^7.1.0",
    "http-proxy-middleware": "^3.0.5",
    "marked": "^9.1.6",
    "multer": "^1.4.5-lts.1",
    "path": "^0.12.7",
    "socket.io": "^4.7.4"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/dompurify": "^3.0.5",
    "@types/express": "^4.17.21",
    "@types/http-proxy-middleware": "^0.19.3",
    "@types/jest": "^29.5.8",
    "@types/marked": "^5.0.2",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
